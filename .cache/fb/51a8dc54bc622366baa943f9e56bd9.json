eturn data.steps.find(\n      step => step.id === parInt(this.$route.params.id)\n      )\n    } */\n  },\n  watch: {\n    '$route.params.id' (to, from){\n      this.step = this.getStep();\n    }\n  },\n/*   mounted(){\n    setTimeout(() => {\n      this.$refs.audio.pause();\n    }, 2000)\n  } */\n};\n</script>\n"},"lineCount":null}},"error":null,"hash":"b62c08bc45db622df94031b49912b848","cacheData":{"env":{}}}